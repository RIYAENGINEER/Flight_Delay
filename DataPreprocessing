//Handling Nan

df.fillna("No Data",inplace=True) #df=df.fillna("No Data") 
df["DelayReason"].unique()

///Output
array(['Weather', 'Air Traffic Control', 'No Data', 'Maintenance'],
      dtype=object)
///

//Handling Dates

df["ScheduledDeparture"]=pd.to_datetime(df["ScheduledDeparture"],format="%Y-%m-%d %H:%M")
df["ActualDeparture"]=pd.to_datetime(df["ActualDeparture"],format="%Y-%m-%d %H:%M")
df["ScheduledArrival"]=pd.to_datetime(df["ScheduledArrival"],format="%Y-%m-%d %H:%M")
df["ActualArrival"]=pd.to_datetime(df["ActualArrival"],format="%Y-%m-%d %H:%M")

//Encoding data

from sklearn.preprocessing import LabelEncoder #scikit learn
df_LableEncoder=LabelEncoder()
for j in df.columns:
  df[j]=df_LableEncoder.fit_transform(df[j])

///Encoded output

